{
  "dashboard": {
    "title": "CS2 Predictions Monitoring",
    "panels": [
      {
        "title": "Prediction Accuracy",
        "type": "graph",
        "targets": [
          { "expr": "rate(tracker_predictions_correct_total[5m]) / rate(tracker_predictions_total[5m])", "legendFormat": "Accuracy" }
        ]
      },
      {
        "title": "Active Predictions",
        "type": "gauge",
        "targets": [ { "expr": "tracker_active_predictions", "legendFormat": "Active" } ]
      },
      {
        "title": "Loop Duration",
        "type": "graph",
        "targets": [ { "expr": "histogram_quantile(0.95, sum(rate(tracker_cycle_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95" } ]
      }
    ]
  }
}
